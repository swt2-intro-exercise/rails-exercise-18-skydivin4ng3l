<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @paper.title %>
</p>

<p>
  <strong>Venue:</strong>
  <%= @paper.venue %>
</p>

<p>
  <strong>Year:</strong>
  <%= @paper.year %>
</p>

<p>
  <strong>Authors:</strong>
  <!--<%= @paper.authors.map{ |author| author.name }.join(', ') %>-->
   <table id= "authors_table">
    <%= @paper.authors.each do |author| %>
      <tr>
        <td><%= author.name %></td>
        <td><%= link_to '#{author.name}', author_path(author) %></td> 
      </tr>
    <% end %>
  </table>
</p>

<%= link_to 'Edit', edit_paper_path(@paper) %> |
<%= link_to 'Back', papers_path %>
